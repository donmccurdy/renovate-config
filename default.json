{
  "extends": [
    "config:base",
    ":disableDependencyDashboard"
  ],
  "timezone": "America/Los_Angeles",
  "schedule": ["* * 1 */3 *"],
  "packageRules": [
    {
      "matchDepTypes": ["dependencies", "devDependencies", "peerDependencies", "optionalDependencies"],
      "enabled": false
    },
    {
      "matchDepTypes": ["action"],
      "automerge": true
    }
  ],
  "lockFileMaintenance": {
    "enabled": false
  },
  "nvm": {
    "enabled": false
  }
}
